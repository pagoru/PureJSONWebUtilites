{
  "header": {
    "content-type": "text/html"
  },
  "osn" : {
    "variables" : {
      "color-black" : "#ccc",
      "color-blue" : "#c3c",
      "heil" : {
        "tag" : "textarea",
        "attributes": {
          "color" : "#{color-black}"
        }
      }
    },
    "content" : [
      {
        "tag": "div",
        "attributes": {
          "margin": 0,
          "border": 0,
          "margin-bottom" : "1px solid #{color-blue}"
        },
        "children" : [
          {
            "tag" : "@{heil}"
          },
          {
            "tag" : "main",
            "children" : [
              {
                "tag" : "h1",
                "attributes" : {
                  "text-align" : "center",
                  "cursor" : "pointer"
                },
                "selectors" : [
                  {
                    "tag" : "hover",
                    "attributes" : {
                      "color" : "#{color-black}"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "hton" : {
    "variables" : {
      "test2": {
        "tag": "a",
        "attributes" : {
          "height" : 41
        },
        "content" : [
          {
            "tag" : "@{test2}",
            "content" : "hola"
          }
        ]
      },
      "test3" : "hola que tal"
    },
    "content" : [
      {
        "tag" : "@{test2}",
        "attributes" : {
          "href" : "asdasd"
        }
      },
      {
        "tag" : "a",
        "content" : "#{test3}? Si, si.\""
      }
    ]
  }
}
