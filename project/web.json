{
  "header": {
    "content-type": "text/html"
  },
  "sson" : {
    "variables" : {
      "color-black" : "#ccc",
      "heil" : {
        "tag" : "textarea",
        "attributes": {
          "color" : "@:color-black"
        }
      }
    },
    "content" : [
      {
        "tag": "div",
        "attributes": {
          "margin": 0,
          "border": 0
        },
        "children" : [
          {
            "tag" : "main",
            "children" : [
              {
                "tag" : "h1",
                "attributes" : {
                  "text-align" : "center",
                  "cursor" : "pointer"
                },
                "selectors" : [
                  {
                    "tag" : "hover",
                    "attributes" : {
                      "color" : "@:color-black"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "hton" : {
    "variables" : {
      "test": {
        "tag": "a",
        "content": "hola"
      },
      "test2": {
        "tag": "a",
        "content": [
          {
            "tag" : "iframe"
          }
        ]
      }
    },
    "content" : [
      {
        "tag" : "head",
        "content" : [
          {
            "tag" : "link",
            "attributes" : {
              "rel" : "stylesheet",
              "type" : "text/css",
              "href" : "http://localhost:8080/css"
            }
          }
        ]
      },
      {
        "tag" : "main",
        "content" : [
          {
            "tag" : "h1",
            "content" : "Hola que tal"
          }
        ]
      },
      {
        "tag" : "@:test2"
      }
    ]
  }
}
